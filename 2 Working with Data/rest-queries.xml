<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<queries xmlns="http://schemas.haulmont.com/cuba/rest-queries.xsd">
    <query entity="clinic_Visit"
           name="recent-visits"
           view="_local">
        <jpql>
            select v from clinic_Visit v order by v.createTs DESC
        </jpql>
        <params/>
    </query>
</queries>